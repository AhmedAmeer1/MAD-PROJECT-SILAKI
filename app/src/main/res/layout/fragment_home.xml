<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment"
    android:background="@color/colorBg"
    android:id="@+id/homeLayout"
    >

    <Button
        android:id="@+id/button10"
        android:layout_width="160dp"
        android:layout_height="154dp"
        android:background="#FFFFFF"
        android:drawableTop="@drawable/skin1"
        android:text="SKIN CARE"
        android:textColor="#48a84c"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toEndOf="@+id/button9"

        app:layout_constraintTop_toBottomOf="@+id/medicine"
        app:layout_constraintVertical_bias="0.115" />

    <View
        android:id="@+id/view"
        android:layout_width="414dp"
        android:layout_height="193dp"
        android:layout_marginStart="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginEnd="5dp"
        android:layout_marginRight="5dp"
        android:background="@drawable/bkimg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"

        />

    <Button
        android:id="@+id/feedback12"
        android:layout_width="160dp"
        android:layout_height="154dp"
        android:background="#FFFFFF"
        android:drawableTop="@drawable/vit"
        android:text="Vitamins  and Nutritions"
        android:textColor="#48a84c"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.347" />

    <Button
        android:id="@+id/button9"
        android:layout_width="160dp"
        android:layout_height="154dp"
        android:background="#FFFFFF"
        android:drawableTop="@drawable/mask1"
        android:text="PERSONAL CARE"
        android:textColor="#48a84c"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.107"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/feedback12"
        app:layout_constraintVertical_bias="0.115" />

    <Button
        android:id="@+id/medicine"
        android:layout_width="160dp"
        android:layout_height="154dp"
        android:background="#FFFFFF"
        android:drawableTop="@drawable/pills"
        android:text="MEDICINES"
        android:textColor="#48a84c"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.515"
        app:layout_constraintStart_toEndOf="@+id/feedback12"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.347" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="244dp"
        android:layout_height="38dp"
        android:bufferType="normal"
        android:text="SRI-PHARMACY"
        android:textAllCaps="true"
        android:textColor="#1E54F6"
        android:textSize="26sp"
        android:textStyle="bold"
        android:typeface="sans"
        app:drawableTint="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.183" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="224dp"
        android:layout_height="195dp"
        android:src="@drawable/q1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.029" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="358dp"
        android:layout_height="121dp"
        android:src="@drawable/footernew"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.509"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.919" />

</androidx.constraintlayout.widget.ConstraintLayout>

